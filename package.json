{
  "name": "tascboard",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "pnpm db:migrate && next build",
    "check": "next lint && tsc --noEmit",
    "db:generate": "drizzle-kit generate",
    "db:drop": "drizzle-kit drop",
    "db:migrate": "tsx ./src/server/db/migrate.ts",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "auth:generate": "pnpx @better-auth/cli generate --config ./src/utils/auth.tsx --output ./src/server/db/auth-schema.ts",
    "dev": "next dev --turbo",
    "email:dev": "email dev --port=3001 --dir=./src/emails",
    "email:build": "email build --dir=./src/emails --packageManager=pnpm",
    "email:start": "email start --dir=./src/emails",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,mdx}\" --cache",
    "format:write": "prettier --write \"**/*.{ts,tsx,js,jsx,mdx}\" --cache",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "preview": "next build && next start",
    "start": "next start",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
